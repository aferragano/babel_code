<%= form_for @original_snippet do |f|%>
<div class="create-snip"
  Title:<br>
  <%= f.text_field :title %><br>
  Description:<br>
  <%= f.text_area :description %><br>
  Code Snippet:<br>
  <%= f.text_area :snippet %><br>
  <%= f.collection_select :language_id, Language.order(:language), :id, :language %><br>
  <%= f.submit "Save", :class => "button" %>
<% end %>
