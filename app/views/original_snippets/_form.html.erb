<%= form_for @original_snippet do |f|%>
  Title:<br>
  <%= f.text_field :title %>
  Description:<br>
  <%= f.text_area :description %>
  Code Snippet:<br>
  <%= f.text_area :snippet %>
  <%= f.collection_select :language_id, Language.order(:language), :id, :language %>
  <%= f.submit "Save" %>
<% end %>
